{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "Lua.workspace.library": [
    "lua",
    "${3rd}/luv/library",
    "${3rd}/vim/library",
    "${3rd}/nvim-api/types",
    "~/.local/share/nvim/lazy/toggleterm.nvim/lua"
  ],
  "Lua.workspace.checkThirdParty": false,
  "Lua.diagnostics.globals": [
    "vim",
    "require",
    "package",
    "io",
    "os",
    "string",
    "table",
    "math",
    "coroutine",
    "debug",
    "pairs",
    "ipairs",
    "print",
    "type",
    "tostring",
    "tonumber",
    "error",
    "pcall",
    "xpcall",
    "select",
    "rawget",
    "rawset",
    "getmetatable",
    "setmetatable",
    "next",
    "unpack",
    "load",
    "loadfile",
    "dofile",
    "assert"
  ],
  "Lua.runtime.version": "LuaJIT",
  "Lua.runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua",
    "?.lua",
    "?/init.lua"
  ],
   "Lua.diagnostics.disable": [
    "lowercase-global",
    "trailing-space"
  ],
}
